n=[]
size=int(input("enter the number of elements:"))

for i in range (0,size):
    str=int(input("enter the elements:"))
    n=n+[str]
total=0
for ele in n :
       total+=ele
mean=total/size
print("mean is:",mean)

total=0
for ele in n:
      total+=(ele-mean)**2

var=total/size
print("variance is:",var)
std=(var)**(1/2)
print("standard deviation is:",std)
